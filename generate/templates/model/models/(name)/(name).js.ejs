steal('core/model', function(){

/**
 * @class <%= name %>
 * @parent core/model
 * @inherits can.Model
 * Wraps <%= namespace %> backend services.
 */
can.Model.extend("<%= Namespace %>.Models.<%= Name %>",
    /* @Static */
    {
        findAll: "GET /api/<%= name %>",
        findOne : "GET /api/<%= name %>/{id}",
        create : "POST /api/<%= name %>",
        update : "PUT /api/<%= name %>/{id}",
        destroy : "DELETE /api/<%= name %>/{id}"
    },
    /* @Prototype */
    {});
});
