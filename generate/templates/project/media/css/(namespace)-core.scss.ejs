@import '<%= namespace %>/media/css/<%= namespace %>-scss';

/*
 * Fonts
 **********************************************************/

@import url('http://fonts.googleapis.com/css?family=Roboto:400,500,700|Roboto+Condensed:400,700&subset=latin,latin-ext');

/*
 * Reset
 **********************************************************/

* {
    margin: 0;
    padding: 0;
}

html, body {
    height: 100%;
}

body {
    min-width: 1060px;
    font-family: $font-family-primary;
    font-size: 12px;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

section {
    display: block;
}

h1, h2, h3, h4, h5, h6 {
    margin: 0 0 10px;
    font-weight: $font-weight-normal;
}

a {
    cursor: pointer;
    outline: 0;
    color: $color-blue-light;
    text-decoration: none;
}

a:hover {
    color: $color-blue-dark;
}

a:focus, a:active {
    color: #333;
}

img {
    vertical-align: middle;
    border: 0;
}

ol, ul {
    list-style: none;
}

p {
    margin: 0 0 10px;
    line-height: 1.5;
    text-align: justify;
}

table {
    border-collapse: collapse;
    width: 100%;
}

button,
input,
select,
textarea {
    font-family: inherit;
    font-size: 100%;
    vertical-align: baseline;
    border: 0;
    outline: 0;
}

button::-moz-focus-inner,
input::-moz-focus-inner {
    border: 0;
    padding: 0;
}

button,
input[type=button],
input[type=submit],
input[type=reset],
input[type=image],
input[type=file] {
    cursor: pointer;
}

textarea {
    overflow: auto;
    vertical-align: top;
    resize: none;
}

input[type=date],
input[type=text],
input[type=email],
input[type=tel],
input[type=password],
input[type=number],
textarea {
    color: #555;
    border: 1px solid #ccc;
    border-radius: $css-radius;
    padding: 6px 7px 8px 7px;
    background-color: #fff;
}

sub, sup {
    line-height: 0;
    position: relative;
    vertical-align: baseline;
}

/*
 * Placeholder
 **********************************************************/

::-webkit-input-placeholder { color: #999; }
:hover::-webkit-input-placeholder { color: #bbb; }
:focus::-webkit-input-placeholder { color: #ddd; }

::-moz-placeholder { color: #999; }
:hover::-moz-placeholder { color: #bbb; }
:focus::-moz-placeholder { color: #ddd; }

:-ms-input-placeholder { color: #999; }
:hover:-ms-input-placeholder { color: #bbb; }
:focus:-ms-input-placeholder { color: #ddd; }

/*
 * Hide IE 10+ Input Controls
 **********************************************************/

::-ms-clear { display: none; }
::-ms-reveal { display: none; }

/*
 * Clearfix
 **********************************************************/

.<%= namespace %>-cf:before, .<%= namespace %>-cf:after,
.<%= namespace %>-grid:before, .<%= namespace %>-grid:after {
    display: table;
    content: "";
    line-height: 0;
}

.<%= namespace %>-cf:after,
.<%= namespace %>-grid:after {
    clear: both;
}

